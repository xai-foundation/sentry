    "eslint": "^8.57.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "nx": "18.1.2",
    "rimraf": "5.0.1",
    "typescript": "^5.5.4",
    "unzipper": "0.10.14"
  },
