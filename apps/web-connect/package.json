{
  "name": "@sentry/web-connect",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --build && vite build && copyfiles -u 1 dist/* dist/**/* ../sentry-client-desktop/public/web",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo && rimraf tsconfig.node.tsbuildinfo && rimraf vite.config.js && rimraf vite.config.d.ts",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:cf:prod": "pnpm build && rm dist/xai-drop-permits.JSON && wrangler pages deploy dist --project-name sentry --branch=main",
    "deploy:cf:dev": "pnpm build && rm dist/xai-drop-permits.JSON && wrangler pages deploy dist --project-name sentry-develop --branch=main"
  },
  "dependencies": {
    "@crossmint/client-sdk-react-ui": "^1.11.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@reown/appkit": "^1.2.0",
    "@reown/appkit-adapter-wagmi": "^1.2.0",
    "@sentry/core": "workspace:*",
    "@sentry/ui": "workspace:*",
    "@tanstack/react-query": "^5.51.15",
    "@wagmi/core": "^2.13.1",
    "axios": "^1.6.2",
    "i18next": "^24.0.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "lodash.isequal": "^4.5.0",
    "react": "^18.2.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.2.0",
    "react-ga4": "2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.1.3",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.16.0",
    "react-toastify": "^10.0.4",
    "tailwindcss": "^3.3.3",
    "wagmi": "^2.12.1"
  },
  "devDependencies": {
    "@types/lodash.isequal": "^4.5.8",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^3.2.0"
  }
}
